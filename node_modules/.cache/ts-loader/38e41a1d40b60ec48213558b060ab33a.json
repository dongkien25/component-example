wB;AAwBnC,EAAA,OAAO,EAAE;AAED,IAAA,aAFC,+BAEuB;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAR,gBAAA,MAAQ,QAAR,MAAQ;AAC5B,gBAAA,MAAM,CAAC,eAAD,EAAkB,WAAW,CAAC,OAA9B,CAAN;AAD4B;AAAA,uBAEL,eAAe,CAAC,WAAhB,EAFK;;AAAA;AAEtB,gBAAA,QAFsB;AAG5B,gBAAA,MAAM,CAAC,eAAD,EAAkB,WAAW,CAAC,IAA9B,CAAN;AACA,gBAAA,MAAM,CAAC,gBAAD,EAAmB,QAAQ,CAAC,IAA5B,CAAN;;AAJ4B;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAK7B,KAPM;AAQD,IAAA,cARC,iCAQsB;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAP,gBAAA,MAAO,SAAP,MAAO;AAC3B,gBAAA,MAAM,CAAC,eAAD,EAAkB,WAAW,CAAC,OAA9B,CAAN;AAD2B;AAAA,uBAEJ,iBAAiB,CAAC,aAAlB,EAFI;;AAAA;AAErB,gBAAA,QAFqB;AAG3B,gBAAA,OAAO,CAAC,GAAR,CAAY,UAAZ,EAAwB,QAAxB;AACA,gBAAA,MAAM,CAAC,eAAD,EAAkB,WAAW,CAAC,IAA9B,CAAN;AACA,gBAAA,MAAM,CAAC,kBAAD,EAAqB,QAAQ,CAAC,IAAT,CAAc,IAAnC,CAAN;;AAL2B;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAM5B;AAdM;AAxB0B,CAAtB,CAAf","sourcesContent":["import axios from \"axios\";\nimport Vue from \"vue\";\nimport Vuex from \"vuex\";\nimport Airline from \"../model/AirlineModel\";\nimport Passenger from '@/model/PassengerModel'\n\nimport airlinesService from '@/services/airlinesService'\nimport passengersService from '@/services/passengersService'\n\nVue.use(Vuex);\n\n\nexport enum LoadingType {\n  IDLE= \"idle\",\n  LOADING = \"loading\",\n  DONE = \"done\",\n  ERROR = \"error\"\n}\n\nexport interface State {\n  num: number\n  airlineList: Airline[]\n  isLoading: LoadingType\n  passengerList: Passenger[] \n}\n\n\nexport default new Vuex.Store<State>({\n  state: {\n    num: 0,\n    airlineList: Array<Airline>(),\n    passengerList : Array<Passenger>(),\n    isLoading: LoadingType.IDLE\n  },\n  mutations: {\n    getListAirline(state: State, arilineList: Airline[]) {\n      state.airlineList = arilineList;\n    },\n    getListPassenger(state: State, passengerList: Passenger[]){\n      state.passengerList = passengerList;\n    },\n    updateLoading(state: State, loading: LoadingType) {\n      state.isLoading = loading\n    },\n    add(state,payload:number) {\n        state.num +=payload   \n    }, \n    minus(state,payload:number) {\n        state.num -=payload\n    }\n  },\n  actions: {\n\n    async fetchAirlines({ commit }) {\n      commit(\"updateLoading\", LoadingType.LOADING)\n      const response = await airlinesService.getAirlines()\n      commit(\"updateLoading\", LoadingType.DONE)\n      commit(\"getListAirline\", response.data);\n    },\n    async fetchPassenger({commit}) {\n      commit(\"updateLoading\", LoadingType.LOADING)\n      const response = await passengersService.getPassengers()\n      console.log('response', response)\n      commit(\"updateLoading\", LoadingType.DONE)\n      commit(\"getListPassenger\", response.data.data);\n    }\n  },\n});\n"],"sourceRoot":""}]}